{
	"name": "generator-mvclight",
	"version": "1.0.0",
	"description": "Small generator using the mvclight express framework",
	"main": "index.js",
	"scripts": {
		"build": "npm run ts-build && npm run tslint",
		"dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run ts-watch\" \"npm run node-watch\"",
		"node-watch": "nodemon dist/app",
		"ts-build": "tsc",
		"ts-watch": "tsc -w",
		"start": "node dist/app",
		"tslint": "tslint -c tslint.json -p tsconfig.json"
	},
	"keywords": [
		"yeoman-generator"
	],
	"files": [
		"generators"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"ejs": "^2.6.1",
		"express": "^4.16.3",
		"inversify": "^4.13.0",
		"mvclight": "^1.0.4",
		"reflect-metadata": "^0.1.12",
		"yeoman-generator": "^3.0.0"
	},
	"devDependencies": {
		"@types/body-parser": "^1.17.0",
		"@types/express": "^4.16.0",
		"concurrently": "^3.6.0",
		"nodemon": "^1.17.5",
		"tslint": "^5.10.0",
		"typescript": "^2.9.2"
	}
}
